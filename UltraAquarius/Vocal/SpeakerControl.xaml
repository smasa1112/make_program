<UserControl x:Class="Vocal.SpeakerControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Vocal"
             mc:Ignorable="d" Height="60" Width="312">
    <Grid>
        <GroupBox Header="Calbration" BorderBrush="Black" BorderThickness="1" HorizontalAlignment="Left" Height="56" VerticalAlignment="Top" Width="312">
            <Grid>
                <ComboBox x:Name="CalibrationList" HorizontalAlignment="Left" VerticalAlignment="Top" Width="220" ItemsSource="{Binding}" SelectionChanged="SelectionChanged" Margin="0,10,0,0"/>
                <Button Content="add" HorizontalAlignment="Left" Margin="225,12,0,0" VerticalAlignment="Top" Width="75" Click="OnAdd"/>
            </Grid>
        </GroupBox>
    </Grid>
</UserControl>
